import "io"

func isValid(parenthesis) {
    var stack = []
    var val
    var cur

    for cur in parenthesis {
        if cur == "(" || cur == "[" || cur == "{" {
            stack.append(cur)
            continue
        }

        val = stack.pop().unwrap_or(nil)

        switch {
            case val == "(" && cur != ")":
                return false

            case val == "[" && cur != "]":
                return false

            case val == "{" && cur != "}":
                return false
        }

        return true
    }
}

var entries = [
    "{]",
    "[](){}",
    "[([()])]{}",
    "[((}))]"
]

var entry
for entry in entries {
    io.print("%s -> %s" % (entry, isValid(entry)))
}
