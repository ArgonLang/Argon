var nterms = integer::new(input("How many terms? "))
var n1 = 0
var n2 = 1
var nth

var count = 0

if nterms <= 0 {
    panic("Please enter a positive integer")
} elif nterms == 1 {
    println(n1)
    return
}

loop count < nterms {
    println(n1)
    nth = n1 + n2
    n1 = n2
    n2 = nth
    count++
}